<div class="text-center login-body">
    <div class="form-signin w-100 m-auto card card-box">
        <form id="login-form">
            <h1 class="h3 mb-3 fw-normal">Welcome</h1>

            <div class="form-floating mb-2">
                <input id="nameLogIn" type="user" class="form-control" id="floatingInput" placeholder="Username"
                    required maxlength="10">
                <label for="floatingInput">Username</label>
            </div>
            <div class="form-floating">
                <input id="pass" type="password" class="form-control" id="floatingPassword" placeholder="Password"
                    required maxlength="8">
                <label for="floatingPassword">Password</label>
            </div>

            <button class="w-50 mt-3 btn btn-sm btn-primary" type="submit">Log in</button>

            <div class="mt-2">
                <a href="/register" class="p-footer">Sing in</a>
            </div>
            
        </form>
    </div>

</div>

<script src="/static/js/logs.js"></script>